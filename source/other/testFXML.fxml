<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="other.TestController">
    <center>
        <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="400.0" prefWidth="600.0">
            <children>
                <Button fx:id="centerButton" mnemonicParsing="false" text="CenterButton"
                        onAction="#clickedCenterButton"/>
            </children>
        </VBox>
    </center>
    <top>
        <Button fx:id="topButton" mnemonicParsing="false" text="Top Button" BorderPane.alignment="CENTER"
                onAction="#clickedTopButton"/>
    </top>
</BorderPane>
